<?xml version="1.0" encoding="utf-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
                                 http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

  <persistence-unit name="etl" transaction-type="RESOURCE_LOCAL">
      <class>com.bcgogo.etl.model.GsmPoint</class>
      <class>com.bcgogo.etl.model.GsmVehicleInfo</class>
      <class>com.bcgogo.etl.model.GsmPointTrace</class>
      <class>com.bcgogo.etl.model.GsmVehicleInfoTrace</class>
      <exclude-unlisted-classes>true</exclude-unlisted-classes>
  </persistence-unit>

</persistence>
