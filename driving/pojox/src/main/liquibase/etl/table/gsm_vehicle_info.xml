            <column name="shrtft1" type="varchar(50)"/>
            <column name="longft1" type="varchar(50)"/>
            <column name="map" type="varchar(50)"/>
            <column name="rpm" type="varchar(50)"/>
            <column name="vss" type="varchar(50)"/>
            <column name="spark_adv" type="varchar(50)"/>
