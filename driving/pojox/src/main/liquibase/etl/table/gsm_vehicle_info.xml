            <column name="rpdtc" type="varchar(1000)"/>
            <column name="upload_time" type="bigint"/>
            <column name="upload_server_time" type="bigint"/>
        </addColumn>
    </changeSet>
    <changeSet id="gsm_vehicle_info_3_sinYuQiu" author="sinYuQiu">
        <createIndex tableName="gsm_vehicle_info" indexName="idx_gsm_vehicle_info_emi">
