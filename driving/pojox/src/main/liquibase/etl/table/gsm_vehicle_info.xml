            <column name="vpwr" type="varchar(50)"/>
            <column name="ife" type="varchar(50)"/>
            <column name="runtime" type="varchar(50)"/>
            <column name="ad_mil" type="varchar(50)"/>
            <column name="ad_feh" type="varchar(50)"/>
            <column name="spwr" type="varchar(50)"/>
            <column name="rdtcn" type="int"/>
            <column name="rdtc" type="varchar(1000)"/>
            <column name="org_info" type="varchar(2000)"/>
            <column name="gsm_vehicle_status" type="varchar(50)"/>
        </createTable>
        <addPrimaryKey tableName="gsm_vehicle_info" columnNames="id" constraintName="pk_gsm_vehicle_info"/>
    </changeSet>
    <changeSet id="gsm_vehicle_info_2_sinYuQiu" author="sinYuQiu">
