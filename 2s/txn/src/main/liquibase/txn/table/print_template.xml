<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet id="print_template_1" author="chenfanglei">
        <createTable tableName="print_template">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="order_type" type="varchar(20)"/>
            <column name="template_html" type="blob"/>
            <column name="shop_id" type="bigint"/>
        </createTable>
    </changeSet>

    <changeSet id="print_template_2" author="chenfanglei">
        <addPrimaryKey tableName="print_template" columnNames="id" constraintName="pk_print_template"/>
    </changeSet>

    <changeSet id="print_template_3" author="chenfanglei">
        <createIndex tableName="print_template" indexName="ipx_print_template_1">
            <column name="shop_id"/>
            <column name="order_type"/>
        </createIndex>
    </changeSet>

    <changeSet id="print_template_4" author="chenfanglei">
        <dropIndex tableName="print_template" indexName="ipx_print_template_1"></dropIndex>
    </changeSet>


    <changeSet id="print_template_5" author="chenfanglei">
        <createIndex tableName="print_template" indexName="ipx_print_template_2">
            <column name="order_type"/>
        </createIndex>
    </changeSet>

    <changeSet id="print_template_6" author="chenfanglei">
        <dropColumn tableName="print_template" columnName="shop_id"></dropColumn>
    </changeSet>

    <changeSet id="print_template_7" author="chenfanglei">
        <addColumn tableName="print_template">
            <column name="name" type="varchar(100)"></column>
        </addColumn>
    </changeSet>

    <changeSet id="print_template_8" author="jimuchen">
        <addColumn tableName="print_template">
            <column name="order_type_enum" type="varchar(50)" />
        </addColumn>
    </changeSet>

  <changeSet author="jimuchen" id="print_template_9">
    <modifyColumn tableName="print_template">
      <column name="version" type="bigint" >
        <constraints nullable="false" />
      </column>
    </modifyColumn>
  </changeSet>

</databaseChangeLog>