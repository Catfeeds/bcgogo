<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="zhangjuntao" id="bcgogo_sms_record_zhangjuntao_1">
        <createTable tableName="bcgogo_sms_record">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="sms_category" type="varchar(50)"/>
            <column name="balance" type="double"/>
            <column name="number" type="bigint"/>
            <column name="recharge_time" type="bigint"/>
            <column name="operate_time" type="bigint"/>
            <column name="operator_id" type="bigint"/>
        </createTable>
        <addPrimaryKey tableName="bcgogo_sms_record" columnNames="id"
                       constraintName="pk_bcgogo_sms_record"/>
    </changeSet>
</databaseChangeLog>