<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="dongnan" id="imported_order_temp_1">
    <createTable tableName="imported_order_temp">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="user_id" type="bigint"/>
      <column name="receipt" type="varchar(150)"/>
      <column name="product_code" type="varchar(150)"/>
      <column name="product_name" type="varchar(150)"/>
      <column name="brand" type="varchar(150)"/>
      <column name="spec" type="varchar(150)"/>
      <column name="model" type="varchar(150)"/>
      <column name="vehicle_model" type="varchar(100)"/>
      <column name="vehicle_brand" type="varchar(100)"/>
      <column name="unit" type="varchar(20)"/>
      <column name="price" type="double"/>
      <column name="amount" type="double"/>
      <column name="order_status" type="varchar(20)"/>
      <column name="order_status_str" type="varchar(20)"/>
      <column name="vest_date" type="varchar(50)"/>
      <column name="customer_supplier_name" type="varchar(150)"/>
      <column name="vehicle" type="varchar(150)"/>

      <column name="contact" type="varchar(50)"/>
      <column name="mobile" type="varchar(50)"/>
      <column name="member_type" type="varchar(50)"/>
      <column name="member_type_str" type="varchar(50)"/>
      <column name="member_card_no" type="varchar(50)"/>
      <column name="pay_per_project" type="varchar(50)"/>
      <column name="sales_man" type="varchar(50)"/>
      <column name="service_total" type="double"/>
      <column name="service_worker" type="varchar(50)"/>
      <column name="service_content" type="varchar(50)"/>
      <column name="in_time" type="varchar(50)"/>
      <column name="out_time" type="varchar(50)"/>
      <column name="pay_way" type="varchar(50)"/>
      <column name="debt" type="double"/>
      <column name="total" type="double"/>
      <column name="memo" type="varchar(500)"/>
      <column name="order_type" type="varchar(20)"/>
      <column name="order_type_str" type="varchar(20)"/>
      <column name="actually_paid" type="double"/>
    </createTable>
  </changeSet>

  <changeSet id="imported_order_temp_2" author="dongnan">
    <addPrimaryKey tableName="imported_order_temp" columnNames="id" constraintName="pk_imported_order_temp"/>
  </changeSet>

  <changeSet author="dongnan" id="imported_order_temp_3">
		<addColumn tableName="imported_order_temp">
			<column name="item_type" type="varchar(70)"/>
		</addColumn>
	</changeSet>
     <changeSet author="dongnan" id="imported_order_temp_4">
		<modifyColumn tableName="imported_order_temp">
			<column name="service_content" type="varchar(100)"/>
		</modifyColumn>
	</changeSet>

    <changeSet id="imported_order_temp_5" author="yangwj">
        <modifyColumn tableName="imported_order_temp">
            <column name="vehicle_brand" type="varchar(200)"/>
            <column name="vehicle_model" type="varchar(200)"/>
        </modifyColumn>
    </changeSet>

</databaseChangeLog>