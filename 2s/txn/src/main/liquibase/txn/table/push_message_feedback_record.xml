<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="xzhu" id="push_message_feedback_record_xzhu_1">
        <createTable tableName="push_message_feedback_record">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="shop_id" type="bigint"/>
            <column name="message_id" type="bigint"/>
            <column name="create_time" type="bigint"/>
            <column name="type" type="varchar(50)"/>

        </createTable>
    </changeSet>

    <changeSet id="push_message_feedback_record_xzhu_2" author="xzhu">
        <addPrimaryKey tableName="push_message_feedback_record" columnNames="id" constraintName="pk_push_message_feedback_record"/>
    </changeSet>
    <changeSet author="ZhangJuntao" id="push_message_2_ZhangJuntao">
        <createIndex tableName="push_message_feedback_record" indexName="IDX_MESSAGE_ID_AND_SHOP_ID">
            <column name="message_id"/>
            <column name="shop_id"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>