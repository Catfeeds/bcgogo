<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="sinYuQiu" id="enquiry_1">
        <createTable tableName="enquiry">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="app_user_no" type="varchar(100)"/>
            <column name="description" type="varchar(500)"/>
            <column name="status" type="varchar(50)"/>
            <column name="bill_id" type="bigint"/>
            <column name="create_time" type="bigint"/>
            <column name="last_update_time" type="bigint"/>
        </createTable>
        <addPrimaryKey tableName="enquiry" columnNames="id" constraintName="pk_enquiry"/>

    </changeSet>
     <changeSet id="enquiry_2" author="SinYuQiu">
       <addColumn tableName="enquiry">
           <column name="app_user_name" type="varchar(50)"/>
           <column name="vehicle_no" type="varchar(20)"/>
           <column name="app_user_mobile" type="varchar(20)"/>
       </addColumn>
     </changeSet>

</databaseChangeLog>

