<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="jimuchen" id="vehicle_serve_month_stat_1">
    <createTable tableName="vehicle_serve_month_stat">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="brand" type="varchar(50)"/>
      <column name="model" type="varchar(50)"/>
      <column name="stat_year" type="int"/>
      <column name="stat_month" type="int"/>
      <column name="wash_times" type="int"/>
      <column name="repair_times" type="int"/>
      <column name="total_times" type="int"/>
      <column name="wash_total" type="numeric(18,1)"/>
      <column name="repair_total" type="numeric(18,1)"/>
      <column name="total_consume" type="numeric(18,1)"/>
    </createTable>

    <addPrimaryKey tableName="vehicle_serve_month_stat" columnNames="id" constraintName="pk_vehicle_serve_month_stat"/>
  </changeSet>

</databaseChangeLog>