<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="yangwj" id="repair_order_service_secondary_1">
        <createTable tableName="repair_order_service_secondary">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="repair_order_secondary_id" type="bigint"/>
            <column name="shop_id" type="bigint"/>
            <column name="service" type="varchar(100)"/>
            <column name="total" type="float"/>
            <column name="workers" type="varchar(500)"/>
            <column name="memo" type="varchar(500)"/>
            <column name="standard_hours" type="float"/>
            <column name="standard_unit_price" type="float"/>
            <column name="actual_hours" type="float"/>
        </createTable>
    </changeSet>
    <changeSet id="repair_order_service_secondary_2" author="yangwj">
        <addPrimaryKey tableName="repair_order_service_secondary" columnNames="id" constraintName="pk_repair_order_service_secondary"/>
    </changeSet>
</databaseChangeLog>