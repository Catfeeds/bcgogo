<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="jimuchen" id="purchase_return_stat_change_1">
    <createTable tableName="purchase_return_stat_change">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="supplier_id" type="bigint"/>
      <column name="product_id" type="bigint"/>
      <column name="stat_year" type="int"/>
      <column name="stat_month" type="int"/>
      <column name="stat_day" type="int"/>
      <column name="times" type="int"/>
      <column name="amount" type="numeric(18,1)"/>
      <column name="total" type="numeric(18,1)"/>
      <column name="stat_time" type="bigint"/>
    </createTable>
  </changeSet>

  <changeSet author="jimuchen" id="purchase_return_stat_change_2">
    <addPrimaryKey tableName="purchase_return_stat_change" columnNames="id" constraintName="pk_purchase_return_stat_change"/>
  </changeSet>

</databaseChangeLog>