<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="qiuxinyu" id="recommend_tree_1">
        <createTable tableName="recommend_tree">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="varchar(50)"/>
            <column name="parent_id" type="bigint"/>
            <column name="sort" type="int"/>
            <column name="image_id" type="bigint"/>
            <column name="status" type="varchar(20)"/>
        </createTable>
        <addPrimaryKey tableName="recommend_tree" columnNames="id" constraintName="pk_recommend_tree_id"/>
    </changeSet>
</databaseChangeLog>
