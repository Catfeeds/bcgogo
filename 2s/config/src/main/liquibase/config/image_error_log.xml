<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="xzhu" id="image_error_log_1">
        <createTable tableName="image_error_log">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="shop_id" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="time" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="code" type="varchar(20)">
                <constraints nullable="false"/>
            </column>
            <column name="message" type="varchar(20)">
                <constraints nullable="false"/>
            </column>

            <column name="url" type="varchar(200)">
                <constraints nullable="false"/>
            </column>
            <column name="content" type="varchar(500)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet author="xzhu" id="image_error_log_2">
        <addPrimaryKey tableName="image_error_log" columnNames="id" constraintName="pk_image_error_log"/>
    </changeSet>

    <changeSet id="image_error_log_3" author="lw">
      <modifyColumn tableName="image_error_log">
        <column name="message" type="varchar(200)"/>
      </modifyColumn>
    </changeSet>


</databaseChangeLog>
