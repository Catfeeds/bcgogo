<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="xzhu" id="image_info_1">
        <createTable tableName="image_info">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="shop_id" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="created_time" type="bigint">
                <constraints nullable="false"/>
            </column>

            <column name="status" type="varchar(20)">
                <constraints nullable="false"/>
            </column>
            <column name="path" type="varchar(500)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet author="xzhu" id="image_info_2">
        <addPrimaryKey tableName="image_info" columnNames="id" constraintName="pk_image_info"/>
    </changeSet>

    <changeSet id="image_info_3_qxy" author="SinYuQiu">
        <addColumn tableName="image_info">
            <column name="app_user_no" type="varchar(50)"/>
        </addColumn>
    </changeSet>

    <changeSet id="image_info_4_zhangjuntao" author="zhangjuntao">
        <modifyColumn tableName="image_info">
            <column name="version" type="bigint" >
                <constraints nullable="false" />
            </column>
            <column name="shop_id" type="bigint" >
                <constraints nullable="true" />
            </column>
        </modifyColumn>
    </changeSet>

</databaseChangeLog>
