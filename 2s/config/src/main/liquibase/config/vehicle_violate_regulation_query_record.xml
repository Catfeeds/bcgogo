<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="lw" id="vehicle_violate_regulation_query_record_1">
    <createTable tableName="vehicle_violate_regulation_query_record" >
      <column name="id" type="bigint"/>
      <column name="version" type="bigint">
          <constraints nullable="false"/>
      </column>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>

      <column name="city" type="varchar(200)"/>
      <column name="vehicle_no" type="varchar(200)"/>

      <column name="query_date" type="bigint"/>

      <column name="result_record" type="varchar(200)"/>
      <column name="reason" type="varchar(200)"/>

    </createTable>
  </changeSet>

  <changeSet id="vehicle_violate_regulation_query_record_2" author="lw">
    <addPrimaryKey tableName="vehicle_violate_regulation_query_record" columnNames="id" constraintName="pk_vehicle_violate_regulation_query_record"/>
  </changeSet>

  <changeSet id="vehicle_violate_regulation_query_record_3" author="lw">
    <addColumn tableName="vehicle_violate_regulation_query_record">
      <column name="query_type" type="varchar(50)"/>
    </addColumn>
  </changeSet>


</databaseChangeLog>
