<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="lw" id="dictionary_1">
        <createTable tableName="dictionary">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="dictionary_name" type="varchar(500)"/>
			<column name="dictionary_version" type="varchar(100)"/>
        </createTable>
        <addPrimaryKey tableName="dictionary" columnNames="id" constraintName="pk_dictionary"/>
    </changeSet>
    <changeSet id="dictionary_2_xinyuQiu" author="xinyuQiu">
        <addColumn tableName="dictionary">
            <column name="is_common" type="varchar(50)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>