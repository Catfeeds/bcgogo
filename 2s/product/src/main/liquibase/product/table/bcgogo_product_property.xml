<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="xzhu" id="bcgogo_product_property_1">
        <createTable tableName="bcgogo_product_property">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="product_id" type="bigint"/>
            <column name="kind" type="varchar(200)"/>
            <column name="type" type="varchar(200)"/>
            <column name="price" type="double"/>
            <column name="image_path" type="varchar(200)"/>
        </createTable>
        <addPrimaryKey tableName="bcgogo_product_property" columnNames="id" constraintName="pk_bcgogo_product_property"/>
    </changeSet>
</databaseChangeLog>