<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="lw" id="shop_vehicle_brand_model_1">
    <createTable tableName="shop_vehicle_brand_model">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="first_letter" type="varchar(50)"/>
      <column name="brand_name" type="varchar(200)"/>
      <column name="model_name" type="varchar(200)"/>
      <column name="brand_id" type="varchar(200)"/>
      <column name="model_id" type="varchar(200)"/>
    </createTable>
    <addPrimaryKey tableName="shop_vehicle_brand_model" columnNames="id" constraintName="pk_shop_vehicle_brand_model"/>
  </changeSet>

  <changeSet author="lw" id="shop_vehicle_brand_model_2">
    <createIndex tableName="shop_vehicle_brand_model" indexName="idx_shopid">
      <column name="shop_id"/>
    </createIndex>
  </changeSet>
  <changeSet id="shop_vehicle_brand_model_3" author="jinyuan">
      <addColumn tableName="shop_vehicle_brand_model">
          <column name="deleted" type="varchar(50)"/>
      </addColumn>
  </changeSet>
</databaseChangeLog>