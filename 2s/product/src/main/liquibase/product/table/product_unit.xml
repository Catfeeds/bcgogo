<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="xzhu" id="product_unit_1">
        <createTable tableName="product_unit">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="product_name" type="varchar(500)"/>
            <column name="unit" type="varchar(100)"/>
            <column name="count" type="bigint"/>
        </createTable>
        <addPrimaryKey tableName="product_unit" columnNames="id" constraintName="pk_product_unit"/>
    </changeSet>
</databaseChangeLog>