<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="luyi" id="product_barcode_1">
        <createTable tableName="product_barcode">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="barcode" type="varchar(20)"/>
            <column name="name" type="varchar(200)"/>
            <column name="brand" type="varchar(200)"/>
            <column name="model" type="varchar(200)"/>
            <column name="spec" type="varchar(2000)"/>
            <column name="product_vehicle_status" type="integer"/>
            <column name="product_vehicle_brand" type="varchar(50)"/>
            <column name="product_vehicle_model" type="varchar(50)"/>
            <column name="product_vehicle_year" type="varchar(10)"/>
            <column name="product_vehicle_engine" type="varchar(10)"/>
        </createTable>
        <addPrimaryKey tableName="product_barcode" columnNames="id" constraintName="pk_product_barcode"/>
    </changeSet>

    <changeSet author="luyi" id="product_barcode_2">
        <addColumn tableName="product_barcode">
            <column name="price" type="double"/>
        </addColumn>
    </changeSet>

  <changeSet author="jimuchen" id="product_barcode_3">
    <modifyColumn tableName="product_barcode">
      <column name="version" type="bigint" >
        <constraints nullable="false" />
      </column>
    </modifyColumn>
  </changeSet>

    <changeSet id="product_barcode_4" author="yangwj">
        <modifyColumn tableName="product_barcode">
            <column name="product_vehicle_brand" type="varchar(200)"/>
            <column name="product_vehicle_model" type="varchar(200)"/>
        </modifyColumn>
    </changeSet>

</databaseChangeLog>