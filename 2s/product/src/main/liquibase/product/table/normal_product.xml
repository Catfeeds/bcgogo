<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="chenfanglei" id="normal_product_1">
        <createTable tableName="normal_product">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="product_category_id" type="varchar(200)"/>
            <column name="commodity_code" type="varchar(70)"/>
            <column name="brand" type="varchar(200)"/>
            <column name="model" type="varchar(200)"/>
            <column name="spec" type="varchar(2000)"/>
            <column name="vehicle_model" type="varchar(200)"/>
            <column name="vehicle_brand" type="varchar(200)"/>
            <column name="unit" type="varchar(200)"/>

        </createTable>
        <addPrimaryKey tableName="normal_product" columnNames="id" constraintName="pk_normal_product"/>
    </changeSet>

    <changeSet id="normal_product_2" author="chenfanglei">
        <addColumn tableName="normal_product">
            <column name="product_name" type="varchar(200)"/>
        </addColumn>
    </changeSet>

    <changeSet id="normal_product_3" author="chenfanglei">
        <addColumn tableName="normal_product">
            <column name="vehicle_brand_id" type="bigint"/>
            <column name="vehicle_model_id" type="bigint"/>
        </addColumn>
    </changeSet>
    <changeSet id="normal_product_4" author="zhangjuntao">
        <createIndex tableName="normal_product" indexName="idx_vehicle_model_id_vehicle_model_id">
            <column name="vehicle_brand_id"/>
            <column name="vehicle_model_id"/>
        </createIndex>
    </changeSet>
    <changeSet id="normal_product_5" author="ndong">
        <addColumn tableName="normal_product">
            <column name="price" type="double"></column>
        </addColumn>
    </changeSet>
    <changeSet id="normal_product_6" author="xzhu">
        <addColumn tableName="normal_product">
            <column name="select_brand_model" type="varchar(50)"></column>
        </addColumn>
    </changeSet>
</databaseChangeLog>