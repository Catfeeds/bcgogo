<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="ZhangJuntao" id="china_pay_param_log_1">
        <createTable tableName="china_pay_param_log">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="ord_id" type="bigint"/>
            <column name="china_pay_scene" type="varchar(50)"/>
            <column name="china_pay_param_status" type="varchar(50)"/>
            <column name="param1" type="varchar(200)"/>
            <column name="param2" type="varchar(200)"/>
            <column name="param3" type="varchar(200)"/>
            <column name="param4" type="varchar(200)"/>
            <column name="param5" type="varchar(200)"/>
            <column name="param6" type="varchar(200)"/>
            <column name="param7" type="varchar(200)"/>
            <column name="param8" type="varchar(200)"/>
            <column name="param9" type="varchar(200)"/>
            <column name="param10" type="varchar(200)"/>
        </createTable>
        <addPrimaryKey tableName="china_pay_param_log" columnNames="id" constraintName="pk_china_pay_param_log"/>
    </changeSet>
    <changeSet id="china_pay_param_log_2" author="xzhu">
        <modifyColumn tableName="china_pay_param_log">
            <column name="param8" type="varchar(500)"/>
            <column name="param9" type="varchar(500)"/>
            <column name="param10" type="varchar(500)"/>
        </modifyColumn>
    </changeSet>
</databaseChangeLog>