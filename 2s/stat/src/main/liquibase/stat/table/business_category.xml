<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="lijinlong" id="business_category_1">
    <createTable tableName="business_category">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="true"/>
      </column>
      <column name="shop_id" type="bigint"/>


      <column name="item_name" type="varchar(50)"/>
      <column name="item_type" type="varchar(50)"/>


    </createTable>
  </changeSet>

  <changeSet author="lijinlong" id="business_category_2">
    <addPrimaryKey tableName="business_category" columnNames="id" constraintName="pk_business_category"/>
  </changeSet>

  <changeSet id="business_category_3" author="lw">
    <addColumn tableName="business_category">
      <column name="use_time" type="bigint"/>
      <column name="money_category" type="varchar(50)"/>
    </addColumn>
  </changeSet>

</databaseChangeLog>
