<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="zhangjuntao" id="app_user_shop_vehicle_1">
        <createTable tableName="app_user_shop_vehicle">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="obd_id" type="bigint"/>
            <column name="app_user_no" type="varchar(100)"/>
            <column name="app_vehicle_id" type="bigint"/>
            <column name="status" type="varchar(50)"/>
            <column name="shop_id" type="bigint"/>
        </createTable>
    </changeSet>

    <changeSet id="app_user_shop_vehicle_2" author="zhangjuntao">
        <addPrimaryKey tableName="app_user_shop_vehicle" columnNames="id" constraintName="pk_app_user_shop_vehicle"/>
    </changeSet>

    <changeSet author="zhangjuntao" id="app_user_shop_vehicle_3">
        <createIndex tableName="app_user_shop_vehicle" indexName="IDX_APP_USER_SHOP_VEHICLE_SHOP_ID">
            <column name="shop_id"/>
        </createIndex>
        <createIndex tableName="app_user_shop_vehicle" indexName="IDX_APP_USER_SHOP_VEHICLE_APP_USER_NO">
            <column name="app_user_no"/>
        </createIndex>
        <createIndex tableName="app_user_shop_vehicle" indexName="IDX_APP_USER_SHOP_VEHICLE_APP_VEHICLE_ID">
            <column name="app_vehicle_id"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>