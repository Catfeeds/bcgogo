<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="weilingfeng" id="user_limit_1">
    <createTable tableName="user_limit">
      <column name="id" type="bigint"/>
      <column name="version" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="name" type="varchar(50)"></column>
      <column name="scene" type="varchar(50)"></column>
      <column name="necessary" type="varchar(20)"></column>
      <column name="type" type="varchar(50)"/>
    </createTable>
    <addPrimaryKey tableName="user_limit" columnNames="id" constraintName="pk_user_limit"/>
  </changeSet>

</databaseChangeLog>