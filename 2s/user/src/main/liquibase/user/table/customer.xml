<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="mazidong" id="customer_1">
    <createTable tableName="customer">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="true"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="code" type="varchar(20)"/>
      <column name="customer_shop_id" type="bigint"/>
      <column name="name" type="varchar(20)"/>
      <column name="company" type="varchar(20)"/>
      <column name="identifier_no" type="varchar(20)"/>
      <column name="gender" type="int"/>
      <column name="birthday" type="bigint"/>
      <column name="mobile" type="varchar(20)"/>
      <column name="landline" type="varchar(20)"/>
      <column name="fax" type="varchar(20)"/>
      <column name="email" type="varchar(50)"/>
      <column name="zip" type="varchar(10)"/>
      <column name="address" type="varchar(50)"/>
      <column name="qq" type="varchar(20)"/>
      <column name="bank" type="varchar(20)"/>
      <column name="account" type="varchar(20)"/>
      <column name="tax_no" type="varchar(50)"/>
      <column name="billing_address" type="bigint"/>
      <column name="invoice_category" type="bigint"/>
      <column name="settlement_type" type="bigint"/>
      <column name="invoice_title" type="varchar(20)"/>
      <column name="dept" type="bigint"/>
      <column name="agent" type="bigint"/>
      <column name="memo" type="varchar(500)"/>
    </createTable>
  </changeSet>

  <changeSet author="mzdong" id="customer_2">
    <dropColumn tableName="customer" columnName="birthday"/>
    <addColumn tableName="customer">
      <column name="birth_year" type="bigint"/>
      <column name="birth_month" type="bigint"/>
      <column name="birth_day" type="bigint"/>
    </addColumn>
  </changeSet>

  <changeSet author="mzdong" id="customer_3">
    <dropColumn tableName="customer" columnName="birth_year"/>
    <dropColumn tableName="customer" columnName="birth_month"/>
    <dropColumn tableName="customer" columnName="birth_day"/>
    <addColumn tableName="customer">
      <column name="birthday" type="bigint"/>
    </addColumn>
  </changeSet>

  <changeSet author="zhouxiaochen" id="customer_4">
