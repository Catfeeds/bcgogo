    </addColumn>
  </changeSet>

  <changeSet author="xiaojian" id="customer_5">
    <addPrimaryKey tableName="customer" columnNames="id" constraintName="pk_customer"/>
  </changeSet>

  <changeSet author="zhouxiaochen" id="customer_6">
    <addColumn tableName="customer">
      <column name="short_name" type="varchar(20)"/>
