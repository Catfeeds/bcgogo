<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="ndong" id="appoint_service_1">
    <createTable tableName="appoint_service">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="true"/>
      </column>
      <column name="shop_id" type="bigint"></column>
      <column name="customer_id" type="bigint"></column>
      <column name="vehicle_id" type="bigint"></column>
      <column name="appoint_name" type="varchar(50)"></column>
      <column name="appoint_date" type="bigint"></column>
      <column name="status" type="varchar(50)"></column>
    </createTable>
  </changeSet>

   <changeSet id="appoint_service_2" author="ndong">
    <addPrimaryKey tableName="appoint_service" columnNames="id" constraintName="pk_appoint_service"/>
  </changeSet>

</databaseChangeLog>