        <column name="name" type="varchar(50)"/>
        <column name="sort" type="bigint"/>
    </createTable>
      <addPrimaryKey tableName="insurance_company" columnNames="id" constraintName="pk_customer"/>
  </changeSet>

</databaseChangeLog>