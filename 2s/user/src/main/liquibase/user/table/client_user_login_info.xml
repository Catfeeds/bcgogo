<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="zhangjuntao" id="client_user_login_info_1">
        <createTable tableName="client_user_login_info">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="shop_id" type="bigint"/>
            <column name="user_id" type="bigint"/>
            <column name="client_version" type="varchar(20)"/>
            <column name="login_time" type="bigint"/>
            <column name="session_id" type="varchar(50)"/>
            <column name="session_create_time" type="bigint"/>
            <column name="logout_time" type="bigint"/>
            <column name="status" type="varchar(50)"/>
        </createTable>
        <addPrimaryKey tableName="client_user_login_info" columnNames="id" constraintName="pk_client_user_login_info"/>
        <createIndex tableName="client_user_login_info" indexName="idx_client_user_login_info_user_id">
            <column name="user_id"/>
        </createIndex>
        <createIndex tableName="client_user_login_info" indexName="idx_client_user_login_info_session_id">
            <column name="session_id"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>