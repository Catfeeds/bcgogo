<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

  <changeSet author="mazidong" id="supplier_1">
    <createTable tableName="supplier">
      <column name="id" type="bigint"/>
      <column name="created" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="last_update" type="bigint">
        <constraints nullable="false"/>
      </column>
      <column name="version" type="bigint">
        <constraints nullable="true"/>
      </column>
      <column name="shop_id" type="bigint"/>
      <column name="code" type="varchar(20)"/>
      <column name="supplier_shop_id" type="bigint"/>
      <column name="name" type="varchar(20)"/>
      <column name="abbr" type="varchar(10)"/>
      <column name="category" type="bigint"/>
      <column name="area_id" type="bigint"/>
      <column name="contact" type="varchar(20)"/>
      <column name="mobile" type="varchar(20)"/>
      <column name="landline" type="varchar(20)"/>
      <column name="fax" type="varchar(20)"/>
      <column name="email" type="varchar(50)"/>
      <column name="zip" type="varchar(10)"/>
      <column name="address" type="varchar(50)"/>
      <column name="qq" type="varchar(20)"/>
      <column name="legal_rep" type="varchar(20)"/>
      <column name="bank" type="varchar(20)"/>
      <column name="account" type="varchar(20)"/>
      <column name="tax_no" type="varchar(50)"/>
      <column name="billing_address" type="varchar(50)"/>
      <column name="invoice_category_id" type="bigint"/>
      <column name="settlement_type_id" type="bigint"/>
      <column name="invoice_title" type="varchar(50)"/>
      <column name="memo" type="varchar(500)"/>
    </createTable>
  </changeSet>

  <changeSet author="mzdong" id="supplier_2">
    <addColumn tableName="supplier">
      <column name="account_name" type="varchar(20)"/>
      <column name="business_scope" type="varchar(500)"/>
    </addColumn>
  </changeSet>

  <changeSet author="xiaojian" id="supplier_3">
    <addPrimaryKey tableName="supplier" columnNames="id" constraintName="pk_supplier"/>
  </changeSet>

  <changeSet author="zhangchuanlong" id="supplier_4">
