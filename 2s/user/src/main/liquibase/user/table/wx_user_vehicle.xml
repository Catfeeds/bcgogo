<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="ndong" id="wx_user_vehicle_1">
        <createTable tableName="wx_user_vehicle">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="open_id" type="varchar(100)"/>
            <column name="vehicle_no" type="varchar(20)"/>
            <column name="vin" type="varchar(20)"/>
            <column name="engine_no" type="varchar(50)"/>
            <column name="deleted" type="varchar(20)"/>
        </createTable>
        <addPrimaryKey tableName="wx_user_vehicle" columnNames="id" constraintName="pk_wx_user_vehicle"/>
    </changeSet>

    <changeSet id="wx_user_vehicle_2" author="ndong" >
        <createIndex tableName="wx_user_vehicle" indexName="IDX_wx_user_vehicle_vehicle_no">
            <column name="vehicle_no"/>
        </createIndex>
    </changeSet>

    <changeSet id="wx_user_vehicle_3" author="ndong">
        <addColumn tableName="wx_user_vehicle">
            <column name="ju_he_city_code" type="varchar(20)"/>
        </addColumn>
    </changeSet>

    <changeSet  id="wx_user_vehicle_4" author="ndong">
        <dropColumn tableName="wx_user_vehicle" columnName="ju_he_city_code"/>
        <addColumn tableName="wx_user_vehicle">
            <column name="province" type="bigint"/>
            <column name="city" type="bigint"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>