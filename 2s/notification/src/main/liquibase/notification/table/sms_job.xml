        </addColumn>
    </changeSet>
    <changeSet id="sms_job_17" author="ndong">
        <addColumn tableName="sms_job">
            <column name="sms_type" type="varchar(50)"/>
        </addColumn>
        <renameColumn tableName="sms_job" oldColumnName="sms_id" newColumnName="sms_send_id" columnDataType="varchar(50)"/>
        <addColumn tableName="sms_job">
            <column name="sms_id" type="bigint" />
        </addColumn>
    </changeSet>

     <changeSet id="sms_job_18_ndong" author="ndong">
        <addColumn tableName="sms_job">
            <column name="customer_id" type="bigint"/>
        </addColumn>
    </changeSet>

    <changeSet id="sms_job_19" author="ndong" >
        <dropColumn tableName="sms_job" columnName="group_id_temp"/>
    </changeSet>

    <changeSet id="sms_job_20_ndong" author="ndong">
        <addColumn tableName="sms_job">
            <column name="app_user_no" type="varchar(50)"/>
        </addColumn>
    </changeSet>

     <changeSet id="sms_job_21_zhangjie" author="zhangjie">
         <modifyColumn tableName="sms_job">
             <column name="receive_mobile" type="varchar(2000)"/>
         </modifyColumn>
     </changeSet>
</databaseChangeLog>