        <class>com.bcgogo.notification.model.MessageSwitch</class>
        <class>com.bcgogo.notification.model.SensitiveWords</class>
        <class>com.bcgogo.notification.model.Announcement</class>
        <class>com.bcgogo.notification.model.Festival</class>
        <class>com.bcgogo.notification.model.UserReadRecord</class>
        <class>com.bcgogo.notification.model.InvitationCode</class>
        <class>com.bcgogo.notification.model.InvitationCodeRecycle</class>
        <class>com.bcgogo.notification.model.SmsSendingTimes</class>
        <class>com.bcgogo.notification.model.PromotionMsgJob</class>
        <class>com.bcgogo.notification.model.WXMsg</class>
