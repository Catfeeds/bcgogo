        <class>com.bcgogo.notification.model.WXSendStatusReport</class>
        <class>com.bcgogo.notification.model.WXShopBill</class>


        <exclude-unlisted-classes>true</exclude-unlisted-classes>
    </persistence-unit>
</persistence>
