                                 http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">

    <persistence-unit name="notification" transaction-type="RESOURCE_LOCAL">

        <class>com.bcgogo.notification.model.ToBox</class>
        <class>com.bcgogo.notification.model.SmsJob</class>
