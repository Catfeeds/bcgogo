<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="qiuxinyu" id="gsm_point_trace_1">
        <createTable tableName="gsm_point_trace">
            <column name="id" type="bigint"/>
            <column name="created" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="last_update" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="emi" type="varchar(50)"/>
            <column name="app_user_no" type="varchar(50)"/>
            <column name="state" type="int"/>
            <column name="app_password" type="varchar(50)"/>
            <column name="gp_group" type="varchar(50)"/>
            <column name="cell_pos" type="varchar(50)"/>
            <column name="lon" type="varchar(50)"/>
            <column name="lon_dir" type="varchar(50)"/>
            <column name="lat" type="varchar(50)"/>
            <column name="lat_dir" type="varchar(50)"/>
            <column name="velocity" type="varchar(50)"/>
            <column name="heading" type="varchar(50)"/>
            <column name="gp_date" type="varchar(20)"/>
            <column name="gp_time" type="varchar(20)"/>
            <column name="org_info" type="varchar(2000)"/>
            <column name="gsm_vehicle_status" type="varchar(50)"/>
            <column name="group_id" type="bigint"/>
            <column name="upload_time" type="bigint"/>
            <column name="upload_server_time" type="bigint"/>
            <column name="gsm_point_type" type="varchar(50)"/>
            <column name="impact_strength" type="int"/>
        </createTable>
        <addPrimaryKey tableName="gsm_point_trace" columnNames="id" constraintName="pk_gsm_point_trace"/>
    </changeSet>
</databaseChangeLog>